@model rastgarproject.viewModel.BaseviewModel
<div class="login" style="direction:rtl">
    <div id="submitdiv" class="login-bottom">
        <h3>ثبت نام</h3>

        <form>
            <div class="sign-up">
                <h4>ایمیل یا شماره موبایل:</h4>
                <input id="signup-email" type="text" value="" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '';}" required="">
            </div>
            <div class="sign-up">
                <h4>رمز عبور</h4>
                <input id="signup-password" type="password" value="" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '';}" required="">

            </div>
            <div class="sign-up">
                <h4>تکرار رمز عبور:</h4>
                <input id="Rsignup-password" type="password" value="" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '';}" required="">

            </div>
            <div class="sign-up" id="VCODE" style="display:none">
                <h4>کد دریافتی :</h4>
                <input id="Veficationcode" type="text" value="" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '';}" required="">

            </div>
            <div class="sign-up">
                @*<input id="subscribesubmit"  type="submit" value="REGISTER NOW">*@
                <a id="subscribesub" class="submit "> ثبت نام</a>
               

                <a id="finalsubmit" class="submit" style="display:none; text-align:center"> تایید</a>
                <a id="subscribesubmit2" class="submit " style="display:none; text-align:center; margin-top:10px"> درخواست ارسال مجدد</a>
            </div>
           
           @* <div class="sign-up" style="display:none" id="VCODE">*@
               

</form>
        <p id="lblsubscribeSubmit" class="alertmess"></p>
    </div>
    <div id="logindiv" class="login-right">
        <h3>ورود</h3>
