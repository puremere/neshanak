            var Phone = $("#signup-email").val();
            var Val = $("#Veficationcode").val();

            if (isValidEmailAddress(Email)) {
                Phone = "";
            }
            if (isValidPhonenumber(Phone)) {
                Email = "";
            }
            var Pass = $("#signup-password").val();

            $('#lblsubscribeSubmit').text("درحال پردازش ...");
            $.ajax({
                url: '/CustomerLogin/CustomerRegister',
                dataType: 'html',
                data: {
                    email: Email,
                    pass: Pass,
                    phone: Phone,
                    val: Val
                },
                success: function (data) {

                    var srt = data.substring(0, data.indexOf("*"));;
                    var URL = data.substring(data.indexOf("*") + 1)

                    if (srt === "3") {
                        $('#lblsubscribeSubmit').text("ایمیل وارد شده موجود است، لطفاً از ایمیل دیگری استفاده کنید");
                    }

                    else if (srt === "2") {
                        $('#lblsubscribeSubmit').text("ایمیل یا شماره تلفن وارد شده موجود است، لطفاً از ایمیل دیگری استفاده کنید");
                    }
                    else if (srt === "5") {
                        $('#lblsubscribeSubmit').text("کد تایید صحیح نیست!");
                    }
                    else {

                        $('#lblsubscribeSubmit').text("ثبت با موفقیت انجام شد");
                        window.location.href = URL;
                    }




                },
                error: function () {
                    $('#lblsubscribeSubmit').text("خطای سیستمی، لطفاً با اپراتور تماس بگیرید");
                }
            });




