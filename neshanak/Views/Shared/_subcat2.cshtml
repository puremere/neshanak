@model neshanak.viewModel.IndexVM

@{
    if (Model.cat != null)
    {
        @Html.DropDownList("subcatid2", new SelectList(Model.cat, "ID", "title"), Resources.res.coosesubcattwo, new { @class = "form-control", id = "subcat2" })

    }
    else
    {
        List<neshanak.viewModel.Cat> lst = new List<neshanak.viewModel.Cat>();
        @Html.DropDownList("subcatid2", new SelectList(lst, "ID", "title"), Resources.res.coosesubcattwo, new { @class = "form-control", id = "subcat2", @disabled = "disabled" })

    }
}


